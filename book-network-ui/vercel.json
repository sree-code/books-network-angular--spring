{
  "version": 2,
  "name": "book-social-network-ui",
  "buildCommand": "npm run build -- --configuration=production",
  "outputDirectory": "dist/book-network-ui",
  "framework": "angular",
  "installCommand": "npm install",
  "devCommand": "npm start",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

